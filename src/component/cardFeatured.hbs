{{#with data}}
<div class="card">
    <div class="card_content">
        <img src="{{photo}}" alt="Featured photo" class="card_image">
        <h2 class="card_title">{{heading}}</h2>
        <p class="card_info">{{info}}</p>
    </div>
    <a href="{{linkUrl}}" class="card_link">{{linkText}}</a>
</div>
{{/with}}